{
  "name": "cc-web",
  "version": "0.0.0",
  "private": true,
  "author": "Karolis Å arapnickis",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} gatsby develop",
    "serve-build": "gatsby serve",
    "try": "rm -rf public && GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} run-s build serve-build",
    "clean": "rm -rf .cache public",
    "lint": "eslint .",
    "format": "prettier '**/*.*' --write",
    "qa": "yarn lint",
    "precommit": "lint-staged",
    "prepush": "yarn qa"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dot-object": "^1.7.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^2.15.20",
    "gatsby-link": "^2.2.15",
    "gatsby-plugin-google-analytics": "^2.1.17",
    "gatsby-plugin-netlify-cms": "^4.1.17",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.15",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-plugin-sitemap": "^2.2.14",
    "gatsby-plugin-styled-components": "^3.1.6",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-remark": "^2.6.24",
    "gatsby-transformer-sharp": "^2.2.16",
    "netlify-cms-app": "^2.9.7",
    "react-burger-menu": "^2.5.2",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.17",
    "react-intl": "^2.4.0",
    "react-lazyload": "^2.3.0",
    "remark-parse": "^5.0.0",
    "styled-components": "^4.3.2",
    "tachyons-sass": "^4.9.3"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "1.2.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-styled-components": "^1.10.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "gatsby-plugin-advanced-sitemap": "^1.4.4",
    "gatsby-plugin-layout": "^1.1.8",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "serve": "^9.2.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
